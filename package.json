{
  "name": "siemens-element",
  "version": "47.6.0",
  "description": "Siemens Element open source theme",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/siemens/element.git"
  },
  "author": {
    "name": "Siemens AG",
    "email": "info@siemens.com"
  },
  "type": "module",
  "engines": {
    "node": "^20.0.0 || ^22.0.0",
    "yarn": "Please use NPM to install dependencies",
    "npm": "10 - 11"
  },
  "commitlint": {
    "extends": [
      "@siemens/commitlint-config/.commitlintrc.js"
    ]
  },
  "prettier": "@siemens/prettier-config/.prettierrc.json",
  "scripts": {
    "start": "ng serve",
    "format": "prettier --log-level warn --write .",
    "lint:format": "prettier --check .",
    "lint:commit": "commitlint --from=origin/main",
    "lint:scss": "stylelint **.scss",
    "lint:ng": "ng lint",
    "lint:playwright:eslint": "eslint playwright",
    "build:examples": "ng build",
    "build:examples:prod": "ng build --configuration production",
    "build:examples:aot": "ng build --configuration aot",
    "build:all": "npm run translate:build && npm run lib:build && npm run live-preview:build && npm run charts:build && npm run native-charts:build",
    "build:all:update-translatable-keys": "npm exec update-translatable-keys",
    "lib:build": "ng build element-ng && cpy \"projects/element-ng/*.scss\" \"./dist/@siemens/element-ng\" && cpy \"{LICENSE.md,README.md}\" \"./dist/@siemens/element-ng/\"",
    "lib:test": "ng test element-ng",
    "lib:test:browser": "cross-env HEADLESS=0 ng test element-ng",
    "translate:test": "ng test element-translate-ng",
    "translate:build": "ng build element-translate-ng && cpy LICENSE.md \"./dist/@siemens/element-translate-ng/\"",
    "docs:build": "uv run mkdocs build --strict",
    "docs:serve": "uv run mkdocs serve",
    "live-preview:build": "ng build live-preview && cpy LICENSE.md \"./dist/@siemens/live-preview/\"",
    "prepare": "husky; exit 0",
    "prepare-brand": "cpy --flat node_modules/@simpl/brand/src/favicon/sie-favicon_internet_64px.png docs/_src --rename=favicon.png && cpy --flat node_modules/@simpl/brand/src/favicon/sie-favicon_internet_64px.png src --rename=favicon.png",
    "postversion": "node ./postversion.js && npm run format",
    "charts:test": "ng test charts-ng",
    "charts:build": "ng build charts-ng && cpy \"{LICENSE.md,projects/charts-ng/README.md}\" \"./dist/@siemens/charts-ng/\"",
    "native-charts:test": "ng test native-charts-ng",
    "native-charts:build": "ng build native-charts-ng && cpy LICENSE.md \"./dist/@siemens/native-charts-ng/\""
  },
  "workspaces": [
    "projects/element-translate-cli",
    "projects/element-translate-ng",
    "projects/element-theme",
    "projects/live-preview",
    "projects/charts-ng",
    "projects/native-charts-ng"
  ],
  "dependencies": {
    "@angular/animations": "19.0.6",
    "@angular/cdk": "19.0.5",
    "@angular/common": "19.0.6",
    "@angular/core": "19.0.6",
    "@angular/forms": "19.0.6",
    "@angular/platform-browser": "19.0.6",
    "@angular/platform-browser-dynamic": "19.0.6",
    "@angular/router": "19.0.6",
    "@ngx-formly/bootstrap": "^6.2.2",
    "@ngx-formly/core": "^6.2.2",
    "@ngx-translate/core": "16.0.4",
    "@siemens/ngx-datatable": "^24.0.0",
    "flag-icons": "7.5.0",
    "google-libphonenumber": "3.2.42",
    "ngx-image-cropper": "^9.0.0",
    "rxjs": "7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-builders/custom-esbuild": "^19.1.0",
    "@angular-devkit/build-angular": "19.0.7",
    "@angular/cli": "19.0.6",
    "@angular/compiler": "19.0.6",
    "@angular/compiler-cli": "19.0.6",
    "@angular/language-service": "19.0.6",
    "@angular/localize": "19.0.6",
    "@axe-core/playwright": "4.10.2",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.31.0",
    "@playwright/test": "1.54.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@siemens/commitlint-config": "2.2.1",
    "@siemens/eslint-config-angular": "2.2.1",
    "@siemens/eslint-plugin-defaultvalue": "2.2.1",
    "@siemens/prettier-config": "2.2.1",
    "@siemens/stylelint-config-scss": "2.2.1",
    "@types/jasmine": "5.1.8",
    "echarts": "5.6.0",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/node": "~22.16.0",
    "angular-eslint": "19.8.1",
    "axe-html-reporter": "2.2.11",
    "cpy-cli": "5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "9.31.0",
    "eslint-plugin-headers": "1.3.3",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-tsdoc": "0.4.0",
    "http-server": "14.1.1",
    "husky": "9.1.7",
    "jasmine-core": "5.8.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-firefox-launcher": "2.1.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "karma-jasmine-seed-reporter": "0.2.0",
    "karma-junit-reporter": "2.0.1",
    "karma-spec-reporter": "0.0.36",
    "ng-packagr": "19.0.1",
    "postcss": "8.5.6",
    "postcss-scss": "4.0.9",
    "prettier": "3.6.2",
    "semantic-release": "^24.2.3",
    "stylelint": "16.21.1",
    "stylelint-scss": "6.12.1",
    "stylelint-use-logical-spec": "5.0.1",
    "typescript": "5.5.3",
    "typescript-eslint": "8.36.0"
  },
  "optionalDependencies": {
    "@simpl/brand": "^0.3.1",
    "@simpl/element-icons": "^1.28.0"
  }
}
