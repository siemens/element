{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "projects": {
    "element-examples": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-esbuild:application",
          "options": {
            "plugins": ["projects/live-preview/esbuild-component-loader.mjs"],
            "outputPath": {
              "base": "dist/element-examples",
              "browser": ""
            },
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "src/tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.png",
              "src/app/examples",
              {
                "glob": "*",
                "input": "projects/element-ng/assets/images",
                "output": "assets/images"
              },
              {
                "glob": "**/*",
                "input": "src/assets/",
                "ignore": ["**/i18n/*.json", "**/i18n/"],
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "./projects/maps-ng/assets",
                "output": "/assets/"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["node_modules/"]
            },
            "styles": ["src/styles.scss"],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "@babel/standalone",
              "google-libphonenumber",
              "pbf",
              "rbush",
              "react-dom/client",
              "vue3-sfc-loader"
            ],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "define": {
              "maptilerKey": "''",
              "maptilerUrl": "''"
            }
          },
          "configurations": {
            "production": {
              "tsConfig": "src/tsconfig.app.prod.json",
              "crossOrigin": "use-credentials",
              "optimization": true,
              "outputHashing": "all",
              "namedChunks": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "local-maps": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local-maps.ts"
                }
              ]
            },
            "aot": {
              "aot": true,
              "sourceMap": false,
              "tsConfig": "src/tsconfig.app.prod.json",
              "outputPath": "dist/element-examples-aot"
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-esbuild:dev-server",
          "options": {
            "buildTarget": "element-examples:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "element-examples:build:production"
            },
            "local": {
              "buildTarget": "element-examples:build:local"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"],
            "eslintConfig": "eslint.config.js"
          }
        }
      }
    },
    "element-ng": {
      "projectType": "library",
      "root": "projects/element-ng",
      "sourceRoot": "projects/element-ng",
      "prefix": "si",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
