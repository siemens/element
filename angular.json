            "allowedCommonJsDependencies": [
              "echarts",
              "gridstack",
              "@angular-architects/module-federation"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
